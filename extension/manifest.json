{
  "manifest_version": 2,
  "name": "Watch with MPV",
  "description": "Open the current web stream with MPV.",
  "version": "1.2.0",
  "icons": {
    "128": "icons/mpv.png"
  },
  "browser_action": {
    "default_icon": "icons/mpv.png"
  },
  "background": {
    "scripts": [ "background.js" ]
  },
  "permissions": [
    "nativeMessaging",
    "activeTab",
    "contextMenus",
    "cookies",
    "*://*/*"
  ]
}
